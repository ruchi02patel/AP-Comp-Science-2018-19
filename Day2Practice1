public class Day2Practice1
{
	public Day2Practice1()
	{
		//8
		for (int i=0; i<5; i++)
		{
			for (int x=(2*i); x<10; x+=2)
			{
				System.out.print(x);
				System.out.print((x+1));
			}
			System.out.println(":"+2*i);
		}
	}

	public static void main(String[]args)
	{
		Day2Practice1 app=new Day2Practice1();
	}
}
